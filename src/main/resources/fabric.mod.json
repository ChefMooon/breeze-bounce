{
	"schemaVersion": 1,
	"id": "${mod_id}",
	"version": "${mod_version}",
	"name": "${mod_name}",
	"description": "${mod_description}",
	"authors": [
		"${mod_authors}"
	],
	"contact": {
		"homepage": "${mod_homepage}",
		"issues": "${mod_issues}",
		"sources": "${mod_github}"
	},
	"license": "${mod_license}",
	"icon": "assets/breezebounce/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
          "com.chefmooon.BreezeBounce"
		],
		"client": [
			"com.chefmooon.BreezeBounceClient"
		],
		"fabric-datagen": [
			"com.chefmooon.data.BreezeBounceDataGenerator"
		]
	},
	"mixins": [
		"breeze-bounce.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.15.11",
		"minecraft": ">=${minecraft_min_version} <=${minecraft_max_version}",
		"java": ">=21",
		"fabric-api": "*"
	},
	"custom": {
		"modmenu": {
			"links": {
				"modmenu.wiki": "${mod_homepage}",
				"modmenu.discord": "${mod_discord}"
			}
		}
	}
}